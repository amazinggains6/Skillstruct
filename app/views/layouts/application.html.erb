<!DOCTYPE html>
<html>
<head>
 	<%= csrf_meta_tag %>
	<%= render 'layouts/stylesheets' %>
	<%= stylesheet_link_tag "http://ajax.googleapis.com/ajax/libs/jqueryui/1.7.2/themes/redmond/jquery-ui.css"%>
	<%= javascript_include_tag :application,  
	"http://ajax.googleapis.com/ajax/libs/jquery/1.4.2/jquery.min.js", "http://ajax.googleapis.com/ajax/libs/jqueryui/1.8.1/jquery-ui.min.js"%>
	  </head>
	  <body>
		<div id = "header_wrapper">
			<%= render 'layouts/header' %>
		</div>
	    <div class="container">
	      <section class="round">
			<% flash.each do |key, value| %>
			 <div class="flash <%= key %>"><%= value %></div>
			<% end %>
	        <%= yield %>
	      </section>
		<%# debug(params) if Rails.env.development? %>
	</div>
	<div id = "footer_wrapper">
	<%= render 'layouts/footer' %>
	</div>
</body>
</html>
